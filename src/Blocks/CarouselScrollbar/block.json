{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "enokh-blocks/carousel-scrollbar",
  "title": "Carousel Scrollbar",
  "category": "enokh-blocks",
  "icon": "details",
  "parent": [
    "enokh-blocks/carousel"
  ],
  "description": "Carousel scrollbar",
  "keywords": [],
  "version": "0.1.0",
  "textdomain": "enokh-blocks",
  "attributes": {
    "uniqueId": {
      "type": "string",
      "default": ""
    },
    "display": {
      "type": "object",
      "default": {
        "display": "",
        "displayTablet": "",
        "displayMobile": "",
        "position": "absolute",
        "positionTablet": "",
        "positionMobile": "",
        "absoluteTop": "auto",
        "absoluteBottom": "4px",
        "absoluteLeft": "",
        "absoluteRight": "",
        "absoluteTopTablet": "",
        "absoluteBottomTablet": "",
        "absoluteLeftTablet": "",
        "absoluteRightTablet": "",
        "absoluteTopMobile": "",
        "absoluteBottomMobile": "",
        "absoluteLeftMobile": "",
        "absoluteRightMobile": ""
      }
    },
    "sizing": {
      "type": "object",
      "default": {
        "width": "100%",
        "height": "4px",
        "drag": "auto"
      }
    },
    "colors": {
      "type": "object",
      "default": {
        "scrollbarBackground": "rgba(0,0,0,.1)",
        "scrollbarDrag": "rgba(0,0,0,.5)"
      }
    },
    "borders": {
      "type": "object",
      "default": {}
    }
  },
  "supports": {
    "className": false,
    "html": false
  },
  "styles": [],
  "editorScript": "file:./editor.tsx",
  "editorStyle": "file:./editor.css",
  "viewScript": "file:./front-office.ts",
  "viewStyle": "file:./front-office.css"
}
