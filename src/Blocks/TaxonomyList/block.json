{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "enokh-universal-theme/taxonomy-list",
  "title": "Enokh Taxonomy List",
  "category": "theme",
  "allowedBlocks": [],
  "icon": "details",
  "description": "",
  "keywords": [],
  "version": "0.1.0",
  "textdomain": "enokh-blocks",
  "attributes": {
    "uniqueId": {
      "type": "string",
      "default": ""
    },
    "inheritQuery": {
      "type": "boolean",
      "default": false
    },
    "taxonomyType": {
      "type": "string",
      "default": "category"
    },
    "showPostCounts": {
      "type": "boolean",
      "default": false
    },
    "showEmpty": {
      "type": "boolean",
      "default": false
    },
    "showOnlyTopLevel": {
      "type": "boolean",
      "default": false
    },
    "showHierarchy": {
      "type": "boolean",
      "default": false
    },
    "query": {
      "type": "object",
      "default": {}
    }
  },
  "providesContext": {
    "enokh-blocks/termQuery": "query",
    "enokh-blocks/taxonomyType": "taxonomyType",
    "enokh-blocks/termQueryId": "uniqueId",
    "enokh-blocks/termInheritQuery": "inheritQuery",
    "enokh-blocks/termShowPostCounts": "showPostCounts"
  },
  "supports": {
    "className": false,
    "html": false
  },
  "styles": [],
  "editorScript": "file:./editor.tsx",
  "editorStyle": "file:./editor.css"
}
