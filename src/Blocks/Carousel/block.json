{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "enokh-blocks/carousel",
  "title": "Carousel",
  "category": "enokh-blocks",
  "allowedBlocks": [],
  "icon": "details",
  "description": "Carousel wrapper",
  "keywords": [],
  "version": "0.1.0",
  "textdomain": "enokh-blocks",
  "attributes": {
    "uniqueId": {
      "type": "string",
      "default": ""
    },
    "variant": {
      "type": "string",
      "default": ""
    },
    "autoplay": {
      "type": "boolean",
      "default": false
    },
    "height": {
      "type": "string",
      "default": "adaptive"
    },
    "heightTablet": {
      "type": "string",
      "default": ""
    },
    "heightMobile": {
      "type": "string",
      "default": ""
    },
    "minHeight": {
      "type": "string",
      "default": "400px"
    },
    "minHeightTablet": {
      "type": "string",
      "default": ""
    },
    "minHeightMobile": {
      "type": "string",
      "default": ""
    },
    "itemPerSlide": {
      "type": "number",
      "default": 1
    },
    "itemPerSlideTablet": {
      "type": "number",
      "default": 1
    },
    "itemPerSlideMobile": {
      "type": "number",
      "default": 1
    },
    "spaceBetween": {
      "type": "number",
      "default": 10
    },
    "spaceBetweenTablet": {
      "type": "number",
      "default": 10
    },
    "spaceBetweenMobile": {
      "type": "number",
      "default": 10
    },
    "spacing": {
      "type": "object",
      "default": {}
    },
    "speed": {
      "type": "number",
      "default": 300
    },
    "autoPlaySpeed": {
      "type": "number",
      "default": 1000
    },
    "autoPlayDelay": {
      "type": "number",
      "default": 3000
    },
    "loop": {
      "type": "boolean",
      "default": false
    },
    "slidesPerGroup": {
      "type": "number",
      "default": 1
    },
    "slidesPerGroupTablet": {
      "type": "number",
      "default": 1
    },
    "slidesPerGroupMobile": {
      "type": "number",
      "default": 1
    },
    "autoItemsPerSlide": {
      "type": "boolean",
      "default": false
    }
  },
  "providesContext": {
    "enokh-blocks/carouselVariant": "variant",
    "enokh-blocks/height": "height",
    "enokh-blocks/heightTablet": "heightTablet",
    "enokh-blocks/heightMobile": "heightMobile",
    "enokh-blocks/minHeight": "minHeight",
    "enokh-blocks/minHeightTablet": "minHeightTablet",
    "enokh-blocks/minHeightMobile": "minHeightMobile",
    "enokh-blocks/carouselItemPerSlide": "itemPerSlide",
    "enokh-blocks/carouselSpaceBetween": "spaceBetween",
    "enokh-blocks/carouselItemPerSlideMobile": "itemPerSlideMobile",
    "enokh-blocks/carouselSpaceBetweenMobile": "spaceBetweenMobile",
    "enokh-blocks/carouselItemPerSlideTablet": "itemPerSlideTablet",
    "enokh-blocks/carouselSpaceBetweenTablet": "spaceBetweenTablet",
    "enokh-blocks/carouselId": "uniqueId"
  },
  "supports": {
    "className": false,
    "html": false
  },
  "styles": [],
  "editorScript": "file:./editor.tsx",
  "editorStyle": "file:./editor.css",
  "viewScript": "file:./front-office.ts",
  "style": "file:./front-office.css"
}
