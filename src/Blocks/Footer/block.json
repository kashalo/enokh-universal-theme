{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "enokh-universal-theme/footer",
  "title": "Enokh Universal Footer",
  "category": "theme",
  "icon": "footer",
  "description": "Footer for the Enokh Universal Theme",
  "keywords": [],
  "version": "0.1.0",
  "textdomain": "enokh-universal-theme",
  "attributes": {
    "templateLock": {
      "type": [ "string", "boolean" ],
      "enum": [ "all", "insert", "contentOnly", false ]
    }
  },
  "providesContext": {},
  "usesContext": [],
  "supports": {
    "className": false,
    "html": false,
    "align":  ["wide", "full"],
    "color": {
      "background": true,
      "__experimentalDefaultControls": {
        "background": true
      }
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true,
      "__experimentalDefaultControls": {
        "margin": true,
        "padding": true,
        "blockGap": true
      }
    },
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true,
      "__experimentalDefaultControls": {
        "color": true,
        "radius": true,
        "style": true,
        "width": true
      }
    },
    "layout": {
      "allowSizingOnChildren": true,
      "layout": {
        "default": {
          "type": "constrained"
        }
      }
    }
  },
  "styles": [],
  "example": {},
  "variations": [],
  "script": "file:./main.ts",
  "style": "file:./main.css",
  "editorScript": "file:./editor.tsx",
  "render": "file:./render.php"
}
