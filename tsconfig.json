{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "removeComments": true,
    "lib": [
      "dom",
      "dom.iterable",
      "es5",
      "es6",
      "es7",
      "es2017"
    ],
    "resolveJsonModule": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "declaration": false,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "jsx": "react",
    "sourceMap": true,
    "noLib": false,
    "downlevelIteration": true
  },
  "compileOnSave": false,
  "exclude": [
    "node_modules",
    "vendor"
  ]
}
