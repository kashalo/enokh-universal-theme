{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "removeComments": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "types": [
      "react",
      "react-dom"
    ],
    "resolveJsonModule": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "declaration": false,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "jsx": "react",
    "jsxFactory": "wp.element.createElement",
    "jsxFragmentFactory": "wp.element.Fragment",
    "sourceMap": true,
    "noLib": false,
    "suppressImplicitAnyIndexErrors": false,
    "downlevelIteration": true,
    "baseUrl": ".",
    "paths": {
      "@enokh-blocks/*": ["resources/ts/*"],
    }
  },
  "compileOnSave": false,
  "exclude": [
    "node_modules",
    "vendor"
  ]
}
