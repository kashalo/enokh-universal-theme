{
  "button": {
    "typography": {
      "fontFamily": "var(--enokh--button--primary--text--family)",
      "fontSize": "var(--enokh--button--primary--text--weight)",
      "fontStyle": "var(--enokh--button--primary--text--style)",
      "fontWeight": "var(--enokh--button--primary--text--weight)"
    },
    "color": {
      "background": "var(--enokh--button--primary--background--color)",
      "text": "var(--enokh--button--primary--text--color)"
    },
    "spacing": {
      "padding": "var(--enokh--button--primary--spacing--padding)"
    },
    "border": {
      "color": "var(--enokh--button--primary--border--color)",
      "width": "var(--enokh--button--primary--border--width)",
      "style": "var(--enokh--button--primary--border--style)",
      "radius": "var(--enokh--button--primary--border--radius)"
    },
    ":hover": {
      "color": {
        "background": "var(--enokh--button--primary--background--color--hover)",
        "text": "var(--enokh--button--primary--text--color--hover)"
      },
      "border": {
        "color": "var(--enokh--button--primary--border--color--hover)",
        "width": "var(--enokh--button--primary--border--width--hover)",
        "style": "var(--enokh--button--primary--border--style--hover)",
        "radius": "var(--enokh--button--primary--border--radius--hover)"
      }
    }
  },
  "heading": {
    "typography": {
      "fontWeight": "var(--enokh--heading--base--text--weight, 300)"
    }
  },
  "h1": {
    "typography": {
      "fontSize": "var(--enokh--h1--base--text--size, calc(2 * var(--wp--preset--font-size--large)))"
    }
  },
  "h2": {
    "typography": {
      "fontSize": "var(--enokh--h2--base--text--size, calc(1.8 * var(--wp--preset--font-size--large)))"
    }
  },
  "h3": {
    "typography": {
      "fontSize": "var(--enokh--h3--base--text--size, calc(1.6 * var(--wp--preset--font-size--large)))"
    }
  },
  "h4": {
    "typography": {
      "fontSize": "var(--enokh--h4--base--text--size, calc(1.4 * var(--wp--preset--font-size--large)))"
    }
  },
  "h5": {
    "typography": {
      "fontSize": "var(--enokh--h5--base--text--size, calc(1.2 * var(--wp--preset--font-size--large)))"
    }
  },
  "h6": {
    "typography": {
      "fontSize": "var(--enokh--h6--base--text--size, calc(1.2 * var(--wp--preset--font-size--large)))"
    }
  },
  "link": {
    "color": {
      "text": "var(--enokh--link--base--text--color)"
    },
    "typography": {
      "textDecoration": "var(--enokh--link--base--text--decoration)"
    },
    ":hover": {
      "color": {
        "text": "var(--enokh--link--base--text--color--hover)"
      },
      "typography": {
        "textDecoration": "var(--enokh--link--base--text--decoration--hover)"
      }
    }
  },
  "caption": {
  },
  "cite": {
    "color": {
      "text": "var(--enokh--cite--text--color)"
    }
  }
}
